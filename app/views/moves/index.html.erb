<p id="notice"><%= notice %></p>

<h1>Tes</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>Comment</th>
      <th>Kihu</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tes.each do |te| %>
      <tr>
        <td><%= te.content %></td>
        <td><%= te.comment %></td>
        <td><%= te.kihu %></td>
        <td><%= link_to 'Show', te %></td>
        <td><%= link_to 'Edit', edit_te_path(te) %></td>
        <td><%= link_to 'Destroy', te, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Te', new_te_path %>
